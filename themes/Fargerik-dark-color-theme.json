{
	"name": "Fargerik Dark",
	"type": "dark",
	"colors": {
		"editor.background": "#1a1a18",
		"editor.foreground": "#fbe6be",
		"activityBarBadge.background": "#cc9600",
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBar.background": "#1d1717",
		"editorGutter.background": "#221e1e",
		"activityBar.background": "#20201f",
		"statusBar.background": "#462f44"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#c385c8"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",
				"variable.function"
			],
			"settings": {
				"foreground": "#36f686"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#fbe6be"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage.type"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ffc115"
			}
		},
		{
			"name": "Storage modifier",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#42f3b8"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#f58a32"
			}
		},
		{
			"name": "Keyword control",
			"scope": [
				"keyword.control",
				"keyword.other.special-method"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#fbca2a"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"meta.tag",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#00eaff"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f1bcbe"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function",
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#75eaee"
			}
		},
		{
			"name": "Special Method",
			"scope": [
				"meta.function-call",
				"support.function"
			],
			"settings": {
				"fontStyle": "",
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#f4f5c5"
			}
		},
		{
			"name": "String, Symbols, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"markup.heading",
				"markup.inserted.git_gutter",
